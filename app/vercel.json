{
  "version": 2,
  "name": "inovalab-frontend",
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist/app/browser",
  "installCommand": "npm install",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

